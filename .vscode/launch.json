            "launchBrowser": {
                "enabled": false
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development",
                "GOOGLE_APPLICATION_CREDENTIALS": "contribution-token-manager.json"
            }
        }
    ]
