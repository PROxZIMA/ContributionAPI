    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Azure",
            "type": "coreclr",
            "request": "launch",
            "requireExactSource": false,
            "console": "integratedTerminal",
            "args": [],
            "preLaunchTask": "Debug build Azure",
            "program": "${workspaceFolder}/src/Contribution.AzureDevOps/bin/Debug/net9.0/Contribution.AzureDevOps.dll",
            "cwd": "${workspaceFolder}/src/Contribution.AzureDevOps/",
            "launchSettingsProfile": "https",
            // "postDebugTask": "Clear debug ENV variable",
            "stopAtEntry": false,
            "launchBrowser": {
                "enabled": false
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            }
        },
        {
            "name": "Debug GitHub",
            "type": "coreclr",
            "request": "launch",
            "requireExactSource": false,
            "console": "integratedTerminal",
            "args": [],
            "preLaunchTask": "Debug build GitHub",
            "program": "${workspaceFolder}/src/Contribution.GitHub/bin/Debug/net9.0/Contribution.GitHub.dll",
            "cwd": "${workspaceFolder}/src/Contribution.GitHub/",
            "launchSettingsProfile": "https",
            // "postDebugTask": "Clear debug ENV variable",
            "stopAtEntry": false,
            "launchBrowser": {
                "enabled": false
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            }
        },
        {
            "name": "Debug Hub",
            "type": "coreclr",
            "request": "launch",
            "requireExactSource": false,
            "console": "integratedTerminal",
            "args": [],
            "preLaunchTask": "Debug build Hub",
            "program": "${workspaceFolder}/src/Contribution.Hub/bin/Debug/net9.0/Contribution.Hub.dll",
            "cwd": "${workspaceFolder}/src/Contribution.Hub/",
            "launchSettingsProfile": "https",
            // "postDebugTask": "Clear debug ENV variable",
